{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "DecisionEngine": {
    "ConfigPath": "Config/DecisionSpecs",
    "DefaultSpecId": "FAMILY_SATURDAY_V1",
    "ApiKey": "dev-api-key-change-in-production"
  }
}
