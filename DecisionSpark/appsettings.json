{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "DecisionEngine": {
    "ConfigPath": "Config/DecisionSpecs",
    "DefaultSpecId": "TECH_STACK_ADVISOR_V1",
    "ApiKey": "dev-api-key-change-in-production"
  },
  "OpenAI": {
    "Provider": "OpenAI",
    "ApiKey": "your-openai-api-key-here",
    "Model": "gpt-4",
    "MaxTokens": 500,
    "Temperature": 0.7,
    "EnableFallback": true,
    "TimeoutSeconds": 30
  },
  "AllowedHosts": "*"
}
